{
  "name": "jupyterlab_bokeh",
  "version": "0.1.0",
  "description": "Bokeh integration for JupyterLab",
  "author": "Luke Canavan",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepublish": "npm run build"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "mimeExtension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.9.0",
    "@jupyterlab/rendermime-interfaces": "^0.3.0",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.5.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.4.0"
  }
}
